#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
#define MAX 500005;
int a[MAX];

int While(int n)
{
  vector<bool> vis(MAX, true);
  while(n!=1)
    {
      if(n % 2 == 0) {n /= 2; vis[n] = false;}
      else {n = n*3+1;}
    }
  return vis;
}
int main()
{
  int n;
  cin >> n;
  int p;
  while(n--)
    {
      cin >> p;
      a.push_back(p);
      While(p);
    }
  sort(a.begin(), a.end(), great<int>());
  int len = a.size();
